<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "https://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <bean id="collection" class="com.selflearning.spring.Test.CollectionDemo1">
       <property name = "list1">
           <list>
           <value>DefaultArrayListIndex1</value>
               <value>DefaultArrayListIndex2</value>
           </list>
       </property>
        <property name = "set1">
            <set>
            <value>DefaultLinkedHashSetIndex1</value>
                <value>DefaultHashSetIndex2</value>
            </set>
        </property>
        <property name = "map1">
            <map>
                <entry key="DefaultMapIndex1" value="LinkedHashMap1"/>
                <entry key="DefaultMapIndex2" value = "LinkedHashMap2"/>
            </map>
        </property>
    </bean>
</beans>